{
  "name": "keshev-web",
  "version": "1.0.0",
  "description": "Client for Keshev Plus",
  "private": true,
  "type": "module",
  "scripts": {
    "proxy-server": "node server/proxy-server.js",
    "dev": "concurrently \"pnpm run server\" \"pnpm run proxy-server\" \"vite\"",
    "start": "vite",
    "reinstall": "pnpm install --no-frozen-lockfile && pnpm run build && mkdir -p dist",
    "build": "rimraf dist && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "bacpd": "scripts\\bacpd.bat"
  },
